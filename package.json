{
  "name": "lego-market-client",
  "version": "0.0.0",
  "scripts": {
    "install-puppeteer": "cd node_modules/puppeteer && npm run install",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npm run install-puppeteer && ng test  --no-watch --no-progress --browsers=ChromeHeadlessCI --code-coverage",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "build:prod": "ng build --prod",
    "start:prod": "http-server ./dist -p 4200",
    "build-and-deploy:prod": "npm run build:prod && npm run start:prod",
    "e2e": "start-server-and-test build-and-deploy:prod http://localhost:4200 cy:run"
  },
  "private": true,
